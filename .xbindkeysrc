# Increase volume
"pamixer -i 10"
	XF86AudioRaiseVolume

# Decrease volume
"pamixer -d 10"
	XF86AudioLowerVolume

# Mute volume
"pamixer -t"
	XF86AudioMute

# Increase backlight
"light -A 20%"
	XF86MonBrightnessUp

# Decrease backlight
"light -U 20%"
	XF86MonBrightnessDown

# Print screen
"scrot '%Y-%m-%d_$wx$h.png' -e 'mv $f ~/Pictures/'"
	Print

